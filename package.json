{
  "name": "one-drive-client",
  "version": "1.0.0",
  "description": "one drive client",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run ts:build",
    "start": "node ./bin/www",
    "lint": "eslint .",
    "test": "mocha ./tests/integrationTest.js --timeout 10000",
    "automatedTest": "mocha ./tests/automatedTest.js --timeout 20000",
    "local": "npm run clean && npm run ts:build && node release/index.js",
    "build:live": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts",
    "build:start": "nodemon --exec npm start",
    "prets:build": "npm run clean",
    "ts:build": "tsc -p . --pretty",
    "ts:build:watch": "npm run ts:build -- -w",
    "clean": "rimraf release/*",
    "predebug": "npm run ts:build",
    "debug": "node --inspect ./bin/www"
  },
  "keywords": [
    "onedrive"
  ],
  "author": "simon.gausmann (@gausim)",
  "license": "ISC",
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "~4.15.2",
    "express-session": "^1.15.2",
    "hbs": "~4.0.1",
    "morgan": "~1.8.1",
    "passport": "^0.3.2",
    "passport-azure-ad": "^3.0.5",
    "serve-favicon": "~2.4.2",
    "superagent": "^3.5.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.0.39",
    "@types/express-session": "^1.15.5",
    "@types/mocha": "^2.2.41",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.2",
    "@types/passport": "^0.3.5",
    "@types/superagent": "^3.5.6",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.12.1",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "^3.3.0",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1"
  }
}
